  function cambiarTextos() {
    var t = setInterval(function (){
      var txtArea = document.getElementsByClassName("slide-menu-toolbar-label")
      if (txtArea.length > 0) {
        clearInterval(t);
        txtArea[0].textContent = "Diapositivas";
        txtArea[1].textContent = "Cerrar";
      }
    })
  }
  cambiarTextos();
  
</script>

