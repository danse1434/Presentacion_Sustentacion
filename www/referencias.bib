issn = {0716-1018},
journal = {Revista chilena de infectolog{\'{i}}a},
keywords = {bacteremia,bacteriemia,bolivariana,colombia,fever,fiebre,hematologic neoplasms,medell{\'{i}}n,neoplasias hematol{\'{o}}gicas,neutropenia,palabras clave,universidad pontificia},
